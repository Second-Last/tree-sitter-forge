{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "froglet",
      "camelcase": "Froglet",
      "title": "Froglet",
      "scope": "source.froglet",
      "path": ".",
      "file-types": [
        "froglet"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^froglet$",
      "class-name": "TreeSitterFroglet"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A more user-friendly Forge, I think.",
    "authors": [
      {
        "name": "Gavin Zhao",
        "email": "gavinz@brown.edu"
      }
    ],
    "links": {
      "repository": "https://github.com/Second-Last/tree-sitter-froglet"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
